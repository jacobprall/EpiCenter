<%= form_with(model: user, local: true) do |form| %>
  <% if user.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(user.errors.count, "error") %> prohibited this user from being saved:</h2>

      <ul>
      <% user.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= form.label :First %>
    <%= form.text_field :fname %>
  </div>

  <div class="field">
    <%= form.label :Last %>
    <%= form.text_field :lname %>
  </div>

  <div class="field">
    <%= form.label :Email %>
    <%= form.text_field :email %>
  </div>

  <div class="field">
    <%= form.label :Password %>
    <%= form.text_field :password %>
  </div>

  <div class="field">
    <%= form.label :Birthdate %>
    <%= form.date_field :dob %>
  </div>

  <div class="field">
    <%= form.label :Gender %>
    <%= form.text_field :gender %>
  </div>

  <div class="field">
    <%= form.label :City %>
    <%= form.text_field :city %>
  </div>

  <div class="field">
    <%= form.label :State %>
    <%= form.text_field :state %>
  </div>

  <div class="field">
    <%= form.label :Country %>
    <%= form.text_field :country %>
  </div>

  <div class="field">
    <%= form.label :Bio %>
    <%= form.text_field :bio %>
  </div>



  <div class="actions">
    <%= form.submit %>
  </div>
<% end %>

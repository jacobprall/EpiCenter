

<header>Sign Up</header>


<%= simple_form_for @user do |f| %>
    <%= f.input :fullname %>
    <%= f.input :age, collection: 14..100 %>
    <%= f.input :email %>
    <%= f.input :password %>
    <%= f.input :city %>
    <%= f.input :state, collection: [ 'AL', 'AK', 'AS', 'AZ', 'AR', 'CA', 'CO', 'CT', 'DE', 'DC', 'FM', 'FL', 'GA', 'GU', 'HI', 'ID', 'IL', 'IN', 'IA', 'KS', 'KY', 'LA', 'ME', 'MH', 'MD', 'MA', 'MI', 'MN', 'MS', 'MO', 'MT', 'NE', 'NV', 'NH', 'NJ', 'NM', 'NY', 'NC', 'ND', 'MP', 'OH', 'OK', 'OR', 'PW', 'PA', 'PR', 'RI', 'SC', 'SD', 'TN', 'TX', 'UT', 'VT', 'VI', 'VA', 'WA', 'WV', 'WI', 'WY' ] %>
    <%= f.input :country, priority: ["United States of America"] %>

    <%= f.button :submit %>
<% end %>
